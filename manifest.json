{
  "manifest_version": 3,
  "name": "EyeRest",
  "version": "1.0",
  "description": "A simple extension to remind you to look away from your screen every 20 minutes.",
  "icons": {
    "16": "images/icon16.svg",
    "48": "images/icon48.svg",
    "128": "images/icon128.svg"
  },
  "permissions": [
    "alarms",
    "tabs",
    "tts",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }

}
